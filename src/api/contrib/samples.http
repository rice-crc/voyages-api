# @name single-voyage
POST {{CurrentHost}}/contrib/data
Accept: application/json
Content-Type: application/json
Accept-Language: en-US,en;q=0.5

{
  "q1": {
    "query": {
      "model": "voyage_voyage",
      "filter": [ { "field": "voyage_id", "value": "1" } ]
    },
    "fields": [ "voyage_id", "dataset" ]
