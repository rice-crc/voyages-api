# @name location-fetch
POST {{CurrentHost}}/contrib/data
Accept: application/json
Content-Type: application/json
Accept-Language: en-US,en;q=0.5

{
  "single": {
    "query": {
      "model": "geo.Location",
      "filter": []
    },
    "fields": [
      "name",
      "value"
    ]
  }
}

